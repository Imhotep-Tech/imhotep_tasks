{
  "expo": {
    "name": "imhotep_tasks_mobile",
    "slug": "imhotep_tasks_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/imhotep_tasks.png",
    "scheme": "imhotep-tasks",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/imhotep_tasks.png",
        "backgroundImage": "./assets/images/imhotep_tasks.png",
        "monochromeImage": "./assets/images/imhotep_tasks.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/imhotep_tasks.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/imhotep_tasks.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "imhotep-tasks.vercel.app",
              "pathPrefix": "/verify-email"
            },
            {
              "scheme": "https",
              "host": "imhotep-tasks.vercel.app",
              "pathPrefix": "/verify-email-change"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "ios": {
      "associatedDomains": ["applinks:imhotep-tasks.vercel.app"]
    }
  }
}
